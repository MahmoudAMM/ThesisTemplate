\begin_layout Standard
Governing Ordinary Differential Equation (
\begin_inset CommandInset nomenclature
LatexCommand nomenclature
symbol "ode"
description "ordinary differential equation"

\end_inset

ode)
\begin_inset Formula 
\begin{equation}
m\ddot{x}(t)+c\dot{x}(t)+kx(t)=f(t)\label{MotherEquation}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Taking Laplace transform, the 
\emph on
ode
\emph default
 is transformed to the algebraic equation
\begin_inset Formula 
\[
m\left(s^{2}X(s)-sx_{0}-\dot{x}_{0}\right)+c\left(sX(s)-x_{0}\right)+kX(s)=F(s)
\]

\end_inset

where 
\begin_inset Formula $x_{0}\equiv x(t=0)$
\end_inset

 and 
\begin_inset Formula $\dot{x}_{0}\equiv\dot{x}(t=0)$
\end_inset

.
\end_layout

\begin_layout Standard
Rearranging yields
\begin_inset Formula 
\begin{equation}
\left(ms^{2}+cs+k\right)X(s)-\left(ms+c\right)x_{0}-m\dot{x}_{0}=F(s)\label{RawGovEqn}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Dividing by 
\begin_inset Formula $m$
\end_inset

 yields
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left(s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}\right)X(s)-\left(s+2\zeta\omega_{\textrm{n}}\right)x_{0}-\dot{x}_{0}=\frac{F(s)}{m}\label{SDOF_eqn}
\end{equation}

\end_inset

where the non-dimensional parameters 
\begin_inset Formula $\omega_{\textrm{n}}$
\end_inset

 and 
\begin_inset Formula $\zeta$
\end_inset

 are the 
\series bold
\emph on
natural frequency
\series default
\emph default
 and 
\series bold
\emph on
damping ratio
\series default
\emph default
 defined as 
\begin_inset Formula 
\begin{equation}
\boxed{\omega_{\textrm{n}}\equiv\sqrt{\frac{k}{m}}}\qquad\&\qquad\boxed{\zeta\equiv\frac{c}{c_{\textrm{c}}}}
\end{equation}

\end_inset

where 
\begin_inset Formula $c_{\textrm{c}}$
\end_inset

 is the 
\series bold
\emph on
critical damping
\series default
\emph default
 defined as
\begin_inset Formula 
\begin{equation}
\boxed{c_{\textrm{c}}\equiv2\sqrt{km}}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
By solving the algebraic equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "SDOF_eqn"

\end_inset

), the response 
\begin_inset Formula $X(s)$
\end_inset

 is obtained as
\begin_inset Formula 
\begin{eqnarray*}
X(s) & = & \frac{F(s)}{m\left(s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}\right)}\\
 &  & +\frac{sx_{0}}{s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}}+\frac{2\zeta\omega_{\textrm{n}}x_{0}+\dot{x}_{0}}{s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}}
\end{eqnarray*}

\end_inset

or
\begin_inset Formula 
\begin{equation}
X(s)=F(s)\,H(s)+\frac{sx_{0}}{s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}}+\frac{2\zeta\omega_{\textrm{n}}x_{0}+\dot{x}_{0}}{s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}}\label{SDOF_Response_s}
\end{equation}

\end_inset

where 
\begin_inset Formula $H(s)$
\end_inset

 is the 
\emph on
Transfer Function
\emph default
 
\begin_inset CommandInset nomenclature
LatexCommand nomenclature
symbol "TF"
description "Transfer Function"

\end_inset

(TF) defined as
\begin_inset Formula 
\begin{flalign}
H(s) & \equiv\frac{\left.X(s)\right|_{\textrm{zero initial conditions}}}{F(s)}\label{FRF_Definition}\\
 & =\frac{1}{ms^{2}+cs+k}\\
 & =\frac{1}{m\left(s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}\right)}\\
 & =\frac{1}{m\left(s-\left(-\zeta\omega_{\textrm{n}}+\omega_{\textrm{n}}\sqrt{\zeta^{2}-1}\right)\right)\left(s-\left(-\zeta\omega_{\textrm{n}}-\omega_{\textrm{n}}\sqrt{\zeta^{2}-1}\right)\right)}
\end{flalign}

\end_inset


\end_layout

\begin_layout Standard
Assuming the roots of 
\begin_inset Formula $H(s)$
\end_inset

 are complex, the TF is written as
\begin_inset Formula 
\begin{equation}
H(s)=\frac{1}{m\left(s-\left(-\zeta\omega_{\textrm{n}}+i\omega_{\textrm{n}}\sqrt{1-\zeta^{2}}\right)\right)\left(s-\left(-\zeta\omega_{\textrm{n}}-i\omega_{\textrm{n}}\sqrt{1-\zeta^{2}}\right)\right)}
\end{equation}

\end_inset

or
\begin_inset Formula 
\begin{equation}
\boxed{H(s)=\frac{1}{m\left(s-\left(-\zeta\omega_{\textrm{n}}+i\omega_{\textrm{d}}\right)\right)\left(s-\left(-\zeta\omega_{\textrm{n}}-i\omega_{\textrm{d}}\right)\right)}}\label{TF}
\end{equation}

\end_inset

where
\begin_inset Formula 
\begin{equation}
\boxed{\omega_{\textrm{d}}\equiv\omega_{\textrm{n}}\sqrt{1-\zeta^{2}}}\label{DampedNaturalFrequency}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Thus the response 
\begin_inset Formula $x(t)$
\end_inset

 can be obtained from equation (
\begin_inset CommandInset ref
LatexCommand ref
reference "SDOF_Response_s"

\end_inset

) as 
\begin_inset Formula 
\begin{flalign}
x(t)= & \mathcal{L}^{-1}\left[X(s)\right]
\end{flalign}

\end_inset

where 
\begin_inset Formula $\mathcal{\mathcal{L}}^{-1}$
\end_inset

 denotes inverse Laplace transform.
\end_layout

\begin_layout Standard
Assuming the TF roots are complex, i.e., 
\begin_inset Formula $\zeta<1$
\end_inset

, inverse Laplace transform tables yield
\begin_inset Formula 
\begin{flalign}
x(t)= & \mathcal{L}^{-1}\left[F(s)\,H(s)\right]\nonumber \\
 & +x_{0}e^{-\zeta\omega_{\textrm{n}}t}\left(\cos\left(\omega_{\textrm{d}}t\right)-\frac{\zeta\omega_{\textrm{n}}}{\omega_{\textrm{d}}}\sin\left(\omega_{\textrm{d}}t\right)\right)\nonumber \\
 & +\left(2\zeta\omega_{\textrm{n}}x_{0}+\dot{x}_{0}\right)\,e^{-\zeta\omega_{\textrm{n}}t}\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\label{SDOF_Response-InverseLaplace-1}
\end{flalign}

\end_inset


\end_layout

\begin_layout Standard
Rearranging yields
\begin_inset Formula 
\begin{equation}
\boxed{\begin{aligned}x(t)= & \mathcal{L}^{-1}\left[F(s)\,H(s)\right]\\
 & +e^{-\zeta\omega_{\textrm{n}}t}\left[x_{0}\cos\left(\omega_{\textrm{d}}t\right)+\left(\zeta\omega_{\textrm{n}}x_{0}+\dot{x}_{0}\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]
\end{aligned}
}\label{SDOF_Response-InverseLaplace}
\end{equation}

\end_inset

or from the convolution property
\begin_inset Formula 
\begin{equation}
\begin{aligned}x(t)= & \left(f*h\right)(t)\\
 & +e^{-\zeta\omega_{\textrm{n}}t}\left[x_{0}\cos\left(\omega_{\textrm{d}}t\right)+\left(\zeta\omega_{\textrm{n}}x_{0}+\dot{x}_{0}\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]
\end{aligned}
\label{SDOF_Response_Duhamel_Integ}
\end{equation}

\end_inset

where 
\begin_inset Formula 
\begin{equation}
\boxed{\begin{aligned}h(t) & \equiv\mathcal{L}^{-1}\left[H(s)\right]=\frac{e^{-\zeta\omega_{\textrm{n}}t}}{m}\,\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\end{aligned}
}\label{ImpulsiveResponse}
\end{equation}

\end_inset


\begin_inset Note Note
status open

\begin_layout Plain Layout
\begin_inset Formula 
\begin{equation}
\boxed{\mathrm{sinc}(x)\equiv\frac{\sin(\pi x)}{\pi x}}
\end{equation}

\end_inset


\end_layout

\end_inset

is the Impulse Response Function 
\begin_inset CommandInset nomenclature
LatexCommand nomenclature
symbol "IRF"
description "Impulse Response Function"

\end_inset

 (IRF), and
\begin_inset Formula 
\begin{flalign}
\left(f*h\right)(t) & \equiv\int_{-\infty}^{\infty}f(\tau)h(t-\tau)\,d\tau\\
 & =\int_{0}^{t}f(\tau)h(t-\tau)\,d\tau\quad,:f(t)=h(t)=0\;\forall t<0
\end{flalign}

\end_inset

is the convolution of 
\begin_inset Formula $f(t)$
\end_inset

 and 
\begin_inset Formula $h(t)$
\end_inset

, assuming stable, linear, physically possible and time invariant system.
\end_layout

\begin_layout Subsection
Inverse Laplace Transform Derivation
\end_layout

\begin_layout Standard
Using Laplace transform property, inverse Laplace can be obtained as
\begin_inset Formula 
\begin{equation}
\frac{\Omega\,s}{\left(s^{2}+\Omega^{2}\right)\left(s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}\right)}\overset{\mathcal{L}}{\Longleftrightarrow}\dot{y}(t)+y(0)\label{ss3}
\end{equation}

\end_inset

where 
\begin_inset Formula $y(t)$
\end_inset

 is the inverse Laplace transform of
\begin_inset Formula 
\[
\frac{\Omega}{\left(s^{2}+\Omega^{2}\right)\left(s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}\right)}
\]

\end_inset

previously derived as 
\begin_inset Formula 
\begin{equation}
y(t)=\frac{-2\zeta r\,\cos\left(\Omega t\right)+\left(1-r^{2}\right)\sin\left(\Omega t\right)+r\,e^{-\zeta\omega_{\textrm{n}}t}\left[2\zeta\,\cos\left(\omega_{\textrm{d}}t\right)+\omega_{\textrm{n}}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]}{\omega_{\textrm{n}}^{2}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Thus
\begin_inset Formula 
\begin{equation}
y(0)=\frac{-2\zeta r+2\zeta r}{\omega_{\textrm{n}}^{2}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}=0\label{ss1}
\end{equation}

\end_inset

and
\begin_inset Formula 
\begin{flalign}
\dot{y}(t)= & \frac{\Omega}{\omega_{\textrm{n}}^{2}}\,\frac{2\zeta r\,\sin\left(\Omega t\right)+\left(1-r^{2}\right)\cos\left(\Omega t\right)}{\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}}+\frac{r}{\omega_{\textrm{n}}^{2}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}\nonumber \\
 & \times\left[\omega_{\textrm{d}}e^{-\zeta\omega_{\textrm{n}}t}\left(-2\zeta\,\sin\left(\omega_{\textrm{d}}t\right)+\omega_{\textrm{n}}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)\frac{\cos\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right)\right.\nonumber \\
 & \hphantom{\times\left[\left(\frac{\cos\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right)\right.}\left.-\zeta\omega_{\textrm{n}}e^{-\zeta\omega_{\textrm{n}}t}\left(2\zeta\,\cos\left(\omega_{\textrm{d}}t\right)+\omega_{\textrm{n}}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right)\right]\nonumber \\
= & \frac{r}{\omega_{\textrm{n}}}\,\frac{\left(1-r^{2}\right)\cos\left(\Omega t\right)+2\zeta r\,\sin\left(\Omega t\right)}{\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}}+\frac{r}{\omega_{\textrm{n}}^{2}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}\nonumber \\
 & \times\left[\vphantom{\left(\frac{\zeta\omega_{\textrm{n}}^{2}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)}{\omega_{\textrm{d}}}\right)}\left(\omega_{\textrm{n}}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)-2\zeta^{2}\omega_{\textrm{n}}\right)\cos\left(\omega_{\textrm{d}}t\right)\right.\nonumber \\
 & \hphantom{\times\left[\left(\frac{\zeta\omega_{\textrm{n}}^{2}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)}{\omega_{\textrm{d}}}\right)\right.}\left.+\left(-2\zeta\omega_{\textrm{d}}-\frac{\zeta\omega_{\textrm{n}}^{2}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)}{\omega_{\textrm{d}}}\right)\sin\left(\omega_{\textrm{d}}t\right)\right]\nonumber \\
= & \frac{r}{\omega_{\textrm{n}}}\,\frac{\left(1-r^{2}\right)\cos\left(\Omega t\right)+2\zeta r\,\sin\left(\Omega t\right)}{\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}}+\frac{re^{-\zeta\omega_{\textrm{n}}t}}{\omega_{\textrm{n}}^{2}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}\nonumber \\
 & \times\left[\vphantom{\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}}-\omega_{\textrm{n}}\left(1-r^{2}\right)\,\cos\left(\omega_{\textrm{d}}t\right)\right.\nonumber \\
 & \hphantom{\times\left[\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right.}\left.+\left(-2\zeta\omega_{\textrm{d}}^{2}-\zeta\omega_{\textrm{n}}^{2}\left(2\zeta^{2}-\left(1-r^{2}\right)\right)\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]\nonumber \\
= & \frac{r}{\omega_{\textrm{n}}}\,\frac{\left(1-r^{2}\right)\cos\left(\Omega t\right)+2\zeta r\,\sin\left(\Omega t\right)}{\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}}+\frac{re^{-\zeta\omega_{\textrm{n}}t}}{\omega_{\textrm{n}}^{2}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}\nonumber \\
 & \times\left[\vphantom{\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}}-\omega_{\textrm{n}}\left(1-r^{2}\right)\,\cos\left(\omega_{\textrm{d}}t\right)\right.\nonumber \\
 & \hphantom{\times\left[\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right.}\left.+\zeta\omega_{\textrm{n}}^{2}\left(-2\left(1-\zeta^{2}\right)-2\zeta^{2}+\left(1-r^{2}\right)\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]\nonumber \\
= & \frac{r}{\omega_{\textrm{n}}}\,\frac{\left(1-r^{2}\right)\cos\left(\Omega t\right)+2\zeta r\,\sin\left(\Omega t\right)}{\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}}+\frac{re^{-\zeta\omega_{\textrm{n}}t}}{\omega_{\textrm{n}}^{2}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}\nonumber \\
 & \times\left[-\omega_{\textrm{n}}\left(1-r^{2}\right)\,\cos\left(\omega_{\textrm{d}}t\right)+\zeta\omega_{\textrm{n}}^{2}\left(-2+\left(1-r^{2}\right)\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]\nonumber \\
= & \frac{r}{\omega_{\textrm{n}}}\,\frac{\left(1-r^{2}\right)\cos\left(\Omega t\right)+2\zeta r\,\sin\left(\Omega t\right)}{\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}}+\frac{re^{-\zeta\omega_{\textrm{n}}t}}{\omega_{\textrm{n}}\left(\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}\right)}\nonumber \\
 & \times\left[-\left(1-r^{2}\right)\,\cos\left(\omega_{\textrm{d}}t\right)-\zeta\omega_{\textrm{n}}\left(1+r^{2}\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]\label{ss2}
\end{flalign}

\end_inset


\end_layout

\begin_layout Standard
Substituting equations (
\begin_inset CommandInset ref
LatexCommand ref
reference "ss1"

\end_inset

) and (
\begin_inset CommandInset ref
LatexCommand ref
reference "ss2"

\end_inset

) in (
\begin_inset CommandInset ref
LatexCommand ref
reference "ss3"

\end_inset

) yields
\begin_inset Formula 
\begin{equation}
\boxed{\begin{aligned}\frac{\Omega\,s}{\left(s^{2}+\Omega^{2}\right)\left(s^{2}+2\zeta\omega_{\textrm{n}}s+\omega_{\textrm{n}}^{2}\right)}\overset{\mathcal{L}}{\Longleftrightarrow}\\
\frac{r}{\omega_{\textrm{n}}}\,\frac{\left(1-r^{2}\right)\cos\left(\Omega t\right)+2\zeta r\,\sin\left(\Omega t\right)-e^{-\zeta\omega_{\textrm{n}}t}\left[\left(1-r^{2}\right)\,\cos\left(\omega_{\textrm{d}}t\right)+\zeta\omega_{\textrm{n}}\left(1+r^{2}\right)\frac{\sin\left(\omega_{\textrm{d}}t\right)}{\omega_{\textrm{d}}}\right]}{\left(1-r^{2}\right)^{2}+\left(2\zeta r\right)^{2}}
\end{aligned}
}\label{Harmonic2}
\end{equation}

\end_inset


\end_layout

